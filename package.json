  "version": "0.1.0",
  "description": "A simple CL tool to sanitize filenames.",
  "main": "index.js",
  "scripts": {
    "hint": "jshint --reporter node_modules/jshint-stylish/stylish.js index.js",
    "dev": "npm run watch",
    "watch": "watchify *.js -o 'npm run hint && test'",
