    "clean"
  ],
  "author": {
    "name": "Matthieu COLLE",
    "url": "https://github.com/raiseandfall"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raiseandfall/sanitize-filenames/issues"
  },
  "homepage": "https://github.com/raiseandfall/sanitize-filenames",
  "devDependencies": {
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "mocha": "*",
    "watchify": "^3.1.0"
  }
}
