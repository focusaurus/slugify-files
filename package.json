{
  "name": "slugify-files",
  "version": "0.2.1",
  "description": "A simple CL tool to slugify files.",
  "scripts": {
    "hint": "jshint --reporter node_modules/jshint-stylish/stylish.js index.js",
    "dev": "npm run watch",
    "test": "mocha tests.js",
    "watch": "watchify *.js -o 'npm run hint && npm run test'",
    "build:js": "npm run hint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/raiseandfall/slugify-files.git"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "slugify": "cli.js"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "file-system",
    "sanitize",
    "slugify",
    "filenames",
    "clean",
    "rename",
    "files",
    "fs",
    "cli",
    "bin",
    "file",
    "glob",
    "urlify"
  ],
  "author": {
    "name": "Matthieu COLLE",
    "url": "https://github.com/raiseandfall"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raiseandfall/slugify-files/issues"
  },
  "homepage": "https://github.com/raiseandfall/slugify-files",
  "devDependencies": {
    "fs-extra": "^0.18.2",
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "mocha": "*",
    "path-exists": "^1.0.0",
    "watchify": "^3.1.0"
  },
  "dependencies": {
    "each-async": "^1.1.1",
    "globby": "^2.0.0",
    "meow": "^3.1.0",
    "object-assign": "^2.0.0",
    "slug": "^0.9.1"
  }
}
